set builder-build-command './build.sh'
plugins-add-dir           '~/tmp_yed/custom_frames'
plugin-load               'custom_frames'

set-custom-buffer-frame    "tmp-0" "s" 0  "h"  "b"  "r"  0.10  0 0 "*test-0"
set-custom-buffer-frame    "tmp-1" "s" 0  "v"  "r"  "r"  0.10  1 0 "*test-1"
set-custom-buffer-frame    "tmp-2" "s" 0  "h"  "t"  "r"  0.10  2 0 "*test-2"
set-custom-buffer-frame    "tmp-3" "s" 0  "v"  "l"  "r"  0.10  3 0 "*test-3"
set-custom-buffer-frame    "tmp-4" "s" 0  "h"  "b"  "r"  0.10  4 0 "*test-4"
set-custom-buffer-frame    "tmp-5" "s" 0  "v"  "r"  "r"  0.10  5 0 "*test-5"
set-custom-buffer-frame    "tmp-6" "s" 0  "h"  "t"  "r"  0.10  6 0 "*test-6"
set-custom-buffer-frame    "tmp-7" "s" 0  "v"  "l"  "r"  0.10  7 0 "*test-7"
set-custom-buffer-frame    "tmp-8" "s" 0  "h"  "b"  "r"  0.10  8 0 "*test-8"
set-custom-buffer-frame    "tmp-9" "s" 0  "v"  "r"  "r"  0.10  9 0 "*test-9"

set-custom-buffer-frame    "ypm-menu"  "f" 1  0.15 0.15 0.70 0.70    0            "*ypm-menu *calc"
set-custom-buffer-frame    "per-right" "s" 0  "v"  "r"  "r"  0.50  2 1  0.30  150 "*log"
set-custom-buffer-frame    "tmp-right" "s" 1  "v"  "r"  "r"  0.50  1 1  0.30  150 "*go-menu"
set-custom-buffer-frame    "tray"      "s" 0  "h"  "b"  "r"  0.25  0 1  0.10  90  "*builder-output"

